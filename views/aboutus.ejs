    <!-- ============================================ -->
    <!-- JavaScript for Timeline Tabs (Section 4) -->
    <!-- ============================================ -->
    <script>
        // Timeline Tab Functionality
        document.addEventListener('DOMContentLoaded', function() {
            const yearTabs = document.querySelectorAll('.year-tab');
            const timelineContents = document.querySelectorAll('.timeline-content');
            
            yearTabs.forEach(tab => {
                tab.addEventListener('click', function() {
                    const selectedYear = this.getAttribute('data-year');
                    
                    // Remove active class from all tabs
                    yearTabs.forEach(t => {
                        t.classList.remove('active', 'text-blue-500');
                        t.classList.add('text-gray-400');
                    });
                    
                    // Add active class to clicked tab
                    this.classList.add('active', 'text-blue-500');
                    this.classList.remove('text-gray-400');
                    
                    // Hide all timeline content
                    timelineContents.forEach(content => {
                        content.classList.remove('active');
                    });
                    
                    // Show selected timeline content
                    const selectedContent = document.getElementById('content-' + selectedYear);
                    if (selectedContent) {
                        selectedContent.classList.add('active');
                    }
                });
            });
        });
    </script>
