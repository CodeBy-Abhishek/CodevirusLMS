    <!-- Custom Styles -->
    <style>
        body {
            font-family: 'Inter', sans-serif;
        }
        
        /* Section 1: Hero Gradient */
        .hero-gradient {
            background: linear-gradient(135deg, 
                #ffffff 0%, 
                #f8fbff 25%, 
                #eef5ff 50%, 
                #e0edff 75%, 
                #d4e5ff 100%
            );
        }
        
        /* Section 3 & 5 & 7: Light Blue Gradient */
        .light-blue-gradient {
            background: linear-gradient(135deg, 
                #f0f9ff 0%, 
                #e8f4fc 25%,
                #f5faff 50%, 
                #e8f4fc 75%,
                #f0f9ff 100%
            );
        }
        
        /* Section 7: CTA Button Gradient */
        .btn-gradient {
            background: linear-gradient(135deg, #3b82f6 0%, #10b981 100%);
        }
        
        .btn-gradient:hover {
            background: linear-gradient(135deg, #2563eb 0%, #059669 100%);
        }
        
        /* Section 4: Timeline Tabs */
        .year-tab {
            position: relative;
            cursor: pointer;
            padding-bottom: 0.75rem;
            transition: color 0.2s ease;
        }
        
        .year-tab::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
            width: 100%;
            height: 2px;
            background-color: transparent;
            transition: background-color 0.2s ease;
        }
        
        .year-tab.active {
            color: #3b82f6;
        }
        
        .year-tab.active::after {
            background-color: #3b82f6;
        }
        
        .timeline-content {
            display: none;
        }
        
        .timeline-content.active {
            display: flex;
        }
    </style>
</head>

